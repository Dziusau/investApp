<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myapp.Controllers.UserMenuPortfolioController">
  <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button alignment="CENTER" layoutX="40.0" layoutY="50.0" mnemonicParsing="false" onAction="#onViewActivesButtonClick" prefHeight="30.0" prefWidth="160.0" text="Активы портфеля" />
            <Button alignment="CENTER" layoutX="40.0" layoutY="200.0" mnemonicParsing="false" onAction="#onDiagramPortfolioButtonClick" prefHeight="30.0" prefWidth="160.0" text="Диграмма структуры " />
            <Button alignment="CENTER" layoutX="40.0" layoutY="150.0" mnemonicParsing="false" onAction="#onGraphicPortfolioButtonClick" prefHeight="30.0" prefWidth="160.0" text="График стоимости" />
            <Button alignment="CENTER" layoutX="40.0" layoutY="300.0" mnemonicParsing="false" onAction="#onExitButtonClick" prefHeight="30.0" prefWidth="160.0" text="назад" />
            <Label fx:id="errorLabel" alignment="CENTER" layoutY="350.0" maxWidth="1.7976931348623157E308" prefHeight="20.0" prefWidth="640.0" textAlignment="CENTER" />
         </children>
      </AnchorPane>
  </children>
</VBox>
