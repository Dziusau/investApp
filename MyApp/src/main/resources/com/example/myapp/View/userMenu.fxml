<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myapp.Controllers.UserMenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button alignment="CENTER" layoutX="245.0" layoutY="100.0" mnemonicParsing="false" onAction="#onProfileButtonClick" prefHeight="30.0" prefWidth="150.0" text="Работа с профилем" textAlignment="CENTER" />
            <Button layoutX="245.0" layoutY="150.0" mnemonicParsing="false" onAction="#onActiveButtonClick" prefHeight="30.0" prefWidth="150.0" text="Работа с активами" />
            <Button layoutX="245.0" layoutY="200.0" mnemonicParsing="false" onAction="#onPortfolioButtonClick" prefHeight="30.0" prefWidth="150.0" text="Работа с портфелем" />
            <Button layoutX="245.0" layoutY="250.0" mnemonicParsing="false" onAction="#onExitButtonClick" prefHeight="30.0" prefWidth="150.0" text="Выход" />
         </children>
    </AnchorPane>
  </children>
</VBox>
